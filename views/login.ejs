<% if(messages.error) {%> <%= messages.error %> <% }%>
<form action="/login" method="post">
	<div>
		<label for="email">Email</label>
		<input class="border-2 border-slate-700" type="text" name="email" id="eml" />
	</div>
	<div>
		<label for="password">Password</label>
		<input class="border-2 border-slate-700" type="password" name="password" id="pwd" />
	</div>
	<button type="submit">Login</button>
</form>
